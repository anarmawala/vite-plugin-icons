    },
    {
      "path": "dist/vite-plugin-icons.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "svg2img": "^0.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anarmawala/vite-plugin-icons"
  },
  "bugs": {
    "url": "https://github.com/anarmawala/vite-plugin-icons/issues",
